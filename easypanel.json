{
  "name": "cursor-ultimate",
  "description": "Editor de c√≥digo completo com Claude AI",
  "type": "app",
  "build": {
    "type": "dockerfile",
    "dockerfile": "Dockerfile"
  },
  "deploy": {
    "port": 80,
    "env": [
      {
        "key": "TZ",
        "value": "America/Sao_Paulo"
      }
    ],
    "resources": {
      "cpu": "0.5",
      "memory": "512Mi"
    },
    "replicas": 1,
    "healthCheck": {
      "enabled": true,
      "path": "/",
      "port": 80,
      "interval": 30,
      "timeout": 5,
      "retries": 3
    }
  },
  "domains": [],
  "volumes": []
}
